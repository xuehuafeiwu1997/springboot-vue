<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.xmy.springbootvuebookdemo.dao.BookDao">    <select id="getAllBooks" resultType="com.xmy.springbootvuebookdemo.entity.Book">        select id,name,author from book    </select>        <insert id="saveBook" parameterType="com.xmy.springbootvuebookdemo.entity.Book">        insert into book(name,author) values(#{name},#{author})    </insert></mapper>